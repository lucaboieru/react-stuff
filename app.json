{
    "appID": "",
    "routes": {
        "/": "index",
        "/home": "home",
        "/home/me": "home",
        "/products": "products",
        "/about": "about",
        "/login": "login",
        "/logout": "logout"
    },
    "pages": {
        "index": {
            "re": "^/$",
            "html": "index.html"
        },
        "home": {
            "re": "^/home$|^/home/me$",
            "html": "index.html"
        },
        "products": {
            "re": "^/products/[^/]+/*$",
            "html": "index.html"
        },
        "about": {
            "re": "^/about$",
            "html": "index.html"
        },
        "login": {
            "re": "^/login$",
            "html": "index.html"
        },
        "logout": {
            "re": "^/logout$",
            "html": "index.html"
        }
    },
    "errors": {
        "not_found": {
            "html": "html/404.html"
        }
    },
    "modules": {
        "products": {
            "operations": {
                "getProducts": {
                    "file": "operations.js"
                },
                "getProduct": {
                    "file": "operations.js"
                }
            }
        },
        "login": {
            "operations": {
                "login": {
                    "file": "operations.js"
                }
            }
        }
    }
}
